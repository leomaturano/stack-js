{
  "editor.rulers": [80, 120],
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "eslint.autoFixOnSave": true,
  "files.associations": {
    "*.njk": "html"
  },
  "files.eol": "\n",
  "prettier.eslintIntegration": true
}
